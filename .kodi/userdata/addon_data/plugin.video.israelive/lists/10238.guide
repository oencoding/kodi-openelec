[{"tvGuide": [{"start": 1454033700, "image": null, "end": 1454040900, "name": "Sgt. Bilko", "description": "Sgt. Bilko is in charge of the Motor Pool at an Army base. He's also a good-natured con man, providing gambling facilities for the soldiers on base. When an old enemy from his past shows up to inspect his records and steal away his fianceé, Sgt. Bilko has to put his skills to creative use..."}, {"start": 1454040900, "image": null, "end": 1454047200, "name": "The Great Outdoors", "description": "A Chicago man's hope for a peaceful family vacation in the woods is shattered when the annoying in-laws drop in."}, {"start": 1454047200, "image": null, "end": 1454052600, "name": "Major Movie Star", "description": "Jessica Simpson a blonde Bubbly movie star, whose career is in a free fall, leaves everything behind and joins the U.S. Army."}, {"start": 1454052600, "image": null, "end": 1454058000, "name": "The Good Girl", "description": "A discount store clerk strikes up an affair with a stock boy who considers himself the incarnation of Holden Caulfield."}, {"start": 1454058000, "image": null, "end": 1454063400, "name": "Hanging Up", "description": "With their 79 year-old dad, Lou, approaching his end after a period of senile and memory loss symptoms, three sisters- Eve, Georgia and Maddy come together once again to find peace with his departure, as well as resolve lingering issues left unspoken…."}, {"start": 1454063400, "image": null, "end": 1454070600, "name": "Cousin Bette", "description": "As her wealthy sister lies on her deathbed, poor seamstress Bette promises to take care of her family and daughter Hortense. When Hortense steals Bette’s love, an aspiring Polish sculptor, Wenceslas, Bette swears vengeance on the Hulot household who have wronged her …."}, {"start": 1454070600, "image": null, "end": 1454077800, "name": "Country Strong", "description": "Emerging songwriter Beau has high hopes for Kelly whom he meets at rehab and her talent as a singer. With Chiles scheduled to open for Kelly and Kelly insisting Beau does the honors, a new duo is born. Soon Kelly is on a downward spiral as Chiles and Beau are climbing…."}, {"start": 1454077800, "image": null, "end": 1454079600, "name": "The Insider", "description": "A spin-off of “Entertainment Tonight,” “The Insider” is a US-syndicated entertainment news program, reporting on tabloid topics involving celebrities and Hollywood events."}, {"start": 1454079600, "image": null, "end": 1454086800, "name": "Remember Me", "description": "A romantic drama centered on two new lovers: Tyler, whose parents have split in the wake of his brother's suicide, and Ally, who lives each day to the fullest since witnessing her mother's murder."}, {"start": 1454086800, "image": null, "end": 1454094000, "name": "Nights in Rodanthe", "description": "A random chance meeting between a doctor and a married woman will change their lives forever."}, {"start": 1454094000, "image": null, "end": 1454101200, "name": "Revolutionary Road", "description": "A young couple living in the suburbs during the 1950s struggle to come to terms with their personal problems while trying to raise their two children.<br/><br/>."}, {"start": 1454101200, "image": null, "end": 1454106600, "name": "Hanging Up", "description": "With their 79 year-old dad, Lou, approaching his end after a period of senile and memory loss symptoms, three sisters- Eve, Georgia and Maddy come together once again to find peace with his departure, as well as resolve lingering issues left unspoken…."}, {"start": 1454106600, "image": null, "end": 1454112900, "name": "Major Movie Star", "description": "Jessica Simpson a blonde Bubbly movie star, whose career is in a free fall, leaves everything behind and joins the U.S. Army."}, {"start": 1454112900, "image": null, "end": 1454121000, "name": "Girl, Interrupted", "description": "On graduating high school Susanna is vexed and ends up taking a bottle of aspirins and a bottle of vodka one night. Diagnosed with (BPD) Borderline Personality Disorder, an ambiguous illness with vague symptoms, she is sent to Claymore mental hospital. There she meets Daisy, a sexually-abused girl with OCD who keeps cutting herself, Georgina the pathological liar, burn victim Polly, and Lisa, the awe-inspiring sociopath..."}, {"start": 1454121000, "image": null, "end": 1454126400, "name": "The Good Girl", "description": "A discount store clerk strikes up an affair with a stock boy who considers himself the incarnation of Holden Caulfield."}, {"start": 1454126400, "image": null, "end": 1454133600, "name": "Mask", "description": "A teenager with a massive facial skull deformity and biker gang mother attempt to live as normal a life as possible under the circumstances."}, {"start": 1454133600, "image": null, "end": 1454139900, "name": "Country Strong", "description": "Emerging songwriter Beau has high hopes for Kelly whom he meets at rehab and her talent as a singer. With Chiles scheduled to open for Kelly and Kelly insisting Beau does the honors, a new duo is born. Soon Kelly is on a downward spiral as Chiles and Beau are climbing…."}, {"start": 1454139900, "image": null, "end": 1454146200, "name": "Nights in Rodanthe", "description": "A random chance meeting between a doctor and a married woman will change their lives forever."}, {"start": 1454146200, "image": null, "end": 1454151600, "name": "Remember Me", "description": "A romantic drama centered on two new lovers: Tyler, whose parents have split in the wake of his brother's suicide, and Ally, who lives each day to the fullest since witnessing her mother's murder."}, {"start": 1454151600, "image": null, "end": 1454158800, "name": "Sweet Liberty", "description": "Michael has written a schollarly book on the revolutionary war. He has sold the film rights. The arrival of the film crew seriously disrupts him as actors want to change their characters, directors want to re-stage battles, and he becomes very infatuated with Faith who will play the female lead in the movie."}, {"start": 1454158800, "image": null, "end": 1454164200, "name": "Cool Dog", "description": "When Jimmy's father gets a promotion, the family must relocate to an apartment in New York City that has a strict \"no pet\" policy. Unwilling to let his master go without a fight, Rainy embarks on a cross-country journey to New York."}, {"start": 1454164200, "image": null, "end": 1454166000, "name": "The Insider", "description": "A spin-off of “Entertainment Tonight,” “The Insider” is a US-syndicated entertainment news program, reporting on tabloid topics involving celebrities and Hollywood events."}, {"start": 1454166000, "image": null, "end": 1454173200, "name": "Punch-Drunk Love", "description": "A shy man with zero romantic experience desperately seeks to win a woman's love.<br/><br/>."}, {"start": 1454173200, "image": null, "end": 1454180400, "name": "Stealing Harvard", "description": "John Plummer tries to make a bold statement to his fiancé, Elaine, and her wealthy father  that he doesn’t need any financial assistance, especially Elaine’s father’s, to secure a home for both him and Elaine. <br/>."}, {"start": 1454180400, "image": null, "end": 1454185800, "name": "Burke and Hare", "description": "A black comedy about two 19th century grave robbers who find a lucrative business providing cadavers for an Edinburgh medical school."}, {"start": 1454185800, "image": null, "end": 1454193000, "name": "Revolutionary Road", "description": "A young couple living in the suburbs during the 1950s struggle to come to terms with their personal problems while trying to raise their two children.<br/><br/>."}, {"start": 1454193000, "image": null, "end": 1454198400, "name": "Nights in Rodanthe", "description": "A random chance meeting between a doctor and a married woman will change their lives forever."}, {"start": 1454198400, "image": null, "end": 1454205600, "name": "Country Strong", "description": "Emerging songwriter Beau has high hopes for Kelly whom 